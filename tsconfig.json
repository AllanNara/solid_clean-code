{
	"compilerOptions": {
		"target": "ES2016",
		"useDefineForClassFields": true,
		"module": "CommonJS",
		"lib": ["ESNext", "DOM"],
		"moduleResolution": "Node",
		"outDir": "./dist",
		"strict": true,
		"removeComments": true,
		// "sourceMap": true,
		"resolveJsonModule": true,
		"esModuleInterop": true,
		// "noEmit": true,
		"noUnusedLocals": false,
		"noUnusedParameters": true,
		"noImplicitReturns": true
	}
}
